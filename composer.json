{
    "name": "nobrainer-web/viewable-dataobject",
    "description": "DataExtension that easily allows a DataObject to be viewed like a Page",
    "authors": [
        {
            "name": "Dynamic",
            "email": "dev@dynamicagency.com",
            "homepage": "http://www.dynamicagency.com"
        }
    ],
    "keywords": [
        "silverstripe", "dataobject", "page", "viewable"
    ],
    "type": "silverstripe-vendormodule",
    "license": "BSD-3-Clause",
    "require": {
        "silverstripe/recipe-cms": "^1.0 || ^5.0",
        "silverstripe/vendor-plugin": "^1.0 || ^5.0"
    },
    "require-dev": {
        "phpunit/PHPUnit": "^5.7",
        "squizlabs/php_codesniffer": "*"
    },
    "autoload": {
        "psr-4": {
            "Dynamic\\ViewableDataObject\\": "src",
            "Dynamic\\ViewableDataObject\\Test\\": "tests"
        }
    },
    "config": {
        "process-timeout": 600
    }
}
